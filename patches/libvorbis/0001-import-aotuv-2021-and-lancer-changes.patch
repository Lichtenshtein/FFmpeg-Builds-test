+STIN void rdft(int n, float *a, int *ip, float *w)
+{
+	int nw, nc;
+	float xi;
+	
+	nw = ip[0];
+	nc = ip[1];
+	if (n > 4) {
+		bitrv2(n, ip + 2, a);
+		cftfsub(n, a);
+		rftfsub(n, a, nc, w);
+	} else if (n == 4) {
+		cftfsub(n, a);
+	}
+	xi = a[0] - a[1];
+	a[0] += a[1];
+	a[1] = xi;
+}
