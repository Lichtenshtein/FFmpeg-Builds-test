+				XMM5	 = _mm_cvtpi32_ps(XMM5, MM6);
+				XMM3	 = _mm_cvtpi32_ps(XMM3, MM7);
+				XMM0	 = _mm_mul_ps(XMM0, PM128(mparm));
+				XMM2	 = _mm_mul_ps(XMM2, PM128(mparm));
+				XMM5	 = _mm_mul_ps(XMM5, PM128(mparm));
+				XMM3	 = _mm_mul_ps(XMM3, PM128(mparm));
