+			LAM1		 = _mm_max_ps(LAM1, XMM3);
+		}
+		Cnt	 = ((n-2)&(~7))+1;
+		for(;j<Cnt;j+=8){
+			__m128	XMM0;
+			{
+				__m128	XMM2;
+				{
+					__m128	XMM1;
+					XMM0	 = _mm_loadu_ps(pcm+j   );
+					XMM1	 = _mm_loadu_ps(pcm+j+ 4);
+					XMM2	 = XMM0;
