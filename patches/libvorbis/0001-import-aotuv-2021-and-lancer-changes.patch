+		XMM2	 = _mm_and_ps(XMM2, PM128(PABSMASK));
+		XMM3	 = _mm_and_ps(XMM3, PM128(PABSMASK));
+		U0.ps	 = XMM0;
+		U1.ps	 = XMM1;
+		U2.ps	 = XMM2;
+		U3.ps	 = XMM3;
+		XMM0	 = _mm_cvtpi32_ps(XMM0, U0.pi64[1]);
+		XMM1	 = _mm_cvtpi32_ps(XMM1, U1.pi64[1]);
