+float lb_loudnoise_fix(const vorbis_look_psy *p,
+                       float noise_compand_level,
+                       const float *logmdct,
+                       const int block_mode,
+                       const int lW_block_mode){
+
+  int i, n=p->n, nq1=p->n25p, nq3=p->n75p;
